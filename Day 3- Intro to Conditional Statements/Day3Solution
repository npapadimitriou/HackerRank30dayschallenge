#include <bits/stdc++.h>

using namespace std;



int main()
{
    int N;
    int a,flag=0;
    cin >> N;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');
    a=N%2;
    if(a==0){
        flag=1;//then even(zigos)
    } 
    if (flag==0){//monos
     cout<<"Weird"<<endl;      
    }
    /*For the if statements followinf in C++ you cannot use 6<=N<=20 but you have to use
    6<=N && N<=20   */
    else{//case of even
        if(N<=5){
            cout<<"Not Weird"<<endl;
        }
        else if(N<=20){
             cout<<"Weird"<<endl;
        }
        else{
            cout<<"Not Weird"<<endl;
        }
    }
    return 0;
}
